{
    "name": "vscode-emacs",
    "displayName": "vscode-emacs",
    "description": "emacs like extension for vscode",
    "version": "0.0.8",
    "publisher": "hiro-sun",
    "homepage": "https://github.com/hiro-sun/vscode-emacs",
    "repository": {
        "type": "git",
        "url": "https://github.com/hiro-sun/vscode-emacs.git"
    },
    "bugs": "https://github.com/hiro-sun/vscode-emacs/issues",
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": [
            {
                "key": "ctrl+f",
                "command": "emacs.C-f",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+b",
                "command": "emacs.C-b",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+n",
                "command": "emacs.C-n",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+p",
                "command": "emacs.C-p",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+a",
                "command": "emacs.C-a",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+e",
                "command": "emacs.C-e",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+f",
                "command": "emacs.M-f",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+b",
                "command": "emacs.M-b",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+v",
                "command": "emacs.C-v",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+v",
                "command": "emacs.M-v",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+shift+.",
                "command": "emacs.M->",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+shift+,",
                "command": "emacs.M-<",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+g g",
                "command": "emacs.M-g_g",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+s",
                "command": "actions.find",
                "when": "!findWidgetVisible"
            },
            {
                "key": "ctrl+s",
                "command": "editor.action.nextMatchFindAction",
                "when": "findWidgetVisible"
            },
            {
                "key": "ctrl+r",
                "command": "actions.find",
                "when": "!findWidgetVisible"
            },
            {
                "key": "ctrl+r",
                "command": "editor.action.previousMatchFindAction",
                "when": "findWidgetVisible"
            },
            {
                "key": "ctrl+d",
                "command": "emacs.C-d",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+h",
                "command": "emacs.C-h",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+d",
                "command": "emacs.M-d",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+k",
                "command": "emacs.C-k",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+w",
                "command": "emacs.C-w",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+w",
                "command": "emacs.M-w",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+y",
                "command": "emacs.C-y",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+j",
                "command": "emacs.C-j",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+m",
                "command": "emacs.C-m",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+x h",
                "command": "emacs.C-x_h",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+x u",
                "command": "emacs.C-x_u",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+/",
                "command": "emacs.C-/",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+x z",
                "command": "emacs.C-x_z",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+;",
                "command": "emacs.C-semicolon",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+;",
                "command": "emacs.M-semicolon",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+g",
                "command": "emacs.C-g",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+g",
                "command": "closeFindWidget",
                "when": "editorFocus && findWidgetVisible"
            },
            {
                "key": "ctrl+g",
                "command": "cancelSelection",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "key": "ctrl+g",
                "command": "closeReferenceSearchEditor",
                "when": "searchViewletVisible"
            },
            {
                "key": "ctrl+space",
                "command": "emacs.C-space",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+x ctrl+f",
                "command": "workbench.action.files.openFolder"
            },
            {
                "key": "ctrl+x ctrl+s",
                "command": "workbench.action.files.save",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+x ctrl+w",
                "command": "workbench.action.files.saveAs",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+p",
                "command": "showPrevParameterHint",
                "when": "editorTextFocus && parameterHintsVisible"
            },
            {
                "key": "ctrl+n",
                "command": "showNextParameterHint",
                "when": "editorTextFocus && parameterHintsVisible"
            },
            {
                "key": "ctrl+p",
                "command": "selectPrevQuickFix",
                "when": "editorFocus && quickFixWidgetVisible"
            },
            {
                "key": "ctrl+n",
                "command": "selectNextQuickFix",
                "when": "editorFocus && quickFixWidgetVisible"
            },
            {
                "key": "ctrl+p",
                "command": "selectPrevSuggestion",
                "when": "editorTextFocus && suggestWidgetVisible"
            },
            {
                "key": "ctrl+n",
                "command": "selectNextSuggestion",
                "when": "editorTextFocus && suggestWidgetVisible"
            },
            {   "key": "ctrl+p",
                "command": "workbench.action.quickOpenNavigatePrevious",
                "when": "inQuickOpen"
            },
            {   "key": "ctrl+n",
                "command": "workbench.action.quickOpenNavigateNext",
                "when": "inQuickOpen"
            },
            {   "key": "ctrl+'",
                "command": "emacs.C-quote",
                "when": "editorTextFocus"
            },
            {   "key": "ctrl+shift+'",
                "command": "emacs.C-doublequote",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
    },
    "devDependencies": {
        "tsd": "^0.6.5",
        "typescript": "^1.6.2",
        "vscode": "0.10.x"
    },
    "dependencies": {
        "lodash": "^4.1.0"
    }
}
